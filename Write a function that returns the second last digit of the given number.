import java.io.*;
import java.util.*;
public class UserMainCode
{
public static void main(int input1){
    if (input1>=0&&input1<10)||(input1<=0&&input1>-10)){
        return -1;
    }
    else if(input1>0) {
        int a= input1/10%10;
        return a;
        
    }
    else{
        int b =input1/10%10*-1;
        return b;
    }
}

